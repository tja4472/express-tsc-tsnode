    "express": "4.17.3"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/node": "14.0.26",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  }
}
